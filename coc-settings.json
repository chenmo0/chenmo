{
    "clangd.path": "clangd-10",
    "clangd.semanticHighlighting": true,
    "coc.preferences.extensionUpdateCheck": "daily",
    "languageserver": {
        "lua": {
            "command": "lua-lsp",
            "filetypes": ["lua"]
        },
        "racket": {
            "command": "racket",
            "args": [
                "--lib",
                "racket-langserver"
            ],
            "filetypes": [
                "scheme"
            ]
        }
    },
    "explorer.file.hiddenRules": {
        "extensions": [
            "meta"
        ]
    }
}
